@model RockBotPanel.Models.Users

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Users</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Chat
        </dt>
        <dd class="col-sm-10">
            @Html.Raw(RockBotPanel.Helpers.TelegramHelper.GetChatName(Model.Chatid.Value))
        </dd>
        <dt class="col-sm-2">
            User ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Userid)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Warns)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Warns)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
